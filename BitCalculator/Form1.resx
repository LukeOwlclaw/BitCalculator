  <data name="BitSwitch.Text" xml:space="preserve">
    <value>//position of first bit:
int l = 0;
//position of second bit:
int r = 0;
//test number
int test = 254;
//left first bit
int a = (1 &lt;&lt; l) &amp; test;
//right (second) bit
int b = (1 &lt;&lt; r) &amp; test;
//move left to right:
a = a &gt;&gt; (l-r);
//move right to left:
b = b &lt;&lt; (l-r);
//erase both positions in test:
test = (test &amp; ~(1 &lt;&lt; l) &amp; ~(1 &lt;&lt; r));
//combine (OR) a,b,test
test = test | a | b;</value>
